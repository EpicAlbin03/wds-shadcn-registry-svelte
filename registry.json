{
	"$schema": "./static/schema/registry.json",
	"name": "wds-shadcn-registry-svelte",
	"homepage": "https://localhost:5173",
	"overrideDependencies": ["vaul-svelte@next"],
	"aliases": {
		"lib": "$lib/registry/lib",
		"ui": "$lib/registry/ui",
		"components": "./components",
		"utils": "$lib/utils",
		"hooks": "$lib/registry/hooks"
	},
	"items": [
		{
			"name": "loading-swap",
			"type": "registry:ui",
			"registryDependencies": ["utils"],
			"files": [
				{
					"type": "registry:file",
					"path": "src\\lib\\registry\\ui\\loading-swap\\index.ts"
				},
				{
					"type": "registry:file",
					"path": "src\\lib\\registry\\ui\\loading-swap\\loading-swap.svelte"
				}
			]
		},
		{
			"name": "action-button",
			"type": "registry:block",
			"registryDependencies": ["button", "loading-swap", "alert-dialog"],
			"files": [
				{
					"type": "registry:component",
					"path": "src\\lib\\registry\\blocks\\action-button.svelte"
				}
			]
		},
		{
			"name": "hello-world",
			"type": "registry:block",
			"registryDependencies": ["button", "utils"],
			"files": [
				{
					"type": "registry:component",
					"path": "src\\lib\\registry\\blocks\\hello-world.svelte"
				}
			]
		}
	]
}
