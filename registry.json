{
	"$schema": "./static/schema/registry.json",
	"name": "wds-shadcn-registry-svelte",
	"homepage": "https://localhost:5173",
	"overrideDependencies": ["vaul-svelte@next"],
	"aliases": {
		"lib": "$lib/registry/lib",
		"ui": "$lib/registry/ui",
		"components": "./components",
		"utils": "$lib/utils",
		"hooks": "$lib/registry/hooks"
	},
	"items": [
		{
			"name": "hello-world",
			"type": "registry:block",
			"registryDependencies": ["button", "utils"],
			"files": [
				{
					"type": "registry:component",
					"path": "src\\lib\\registry\\blocks\\hello-world.svelte"
				}
			]
		}
	]
}
